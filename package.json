{
  "name": "gll-frontend-server",
  "version": "0.0.1",
  "description": "Server for G&L frontend website",
  "repository": {
    "type": "git",
    "url": "https://github.com/madcatmk2/greatLiaison-frontend.git"
  },
  "main": "server.js",
  "dependencies": {
    "babelify": "^6.3.0",
    "body-parser": "^1.4.3",
    "express": "^4.4.5",
    "flux": "^2.0.0",
    "jquery": "^2.2.2",
    "keymirror": "^0.1.1",
    "marked": "^0.3.5",
    "object-assign": "^4.1.0",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-router": "^2.0.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "envify": "^3.4.0",
    "watchify": "^3.4.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle-prod": "watchify public/js/app.js -v -t babelify -t [ envify --NODE_ENV production ] -o public/js/bundle.js",
    "bundle-dev": "watchify public/js/app.js -v -t babelify -t [ envify --NODE_ENV development ] -o public/js/bundle.js",
    "start": "npm run bundle-dev & node server.js",
    "prod-start": "npm run bundle-prod & node server.js"
  },
  "keywords": [
    "great",
    "liaison",
    "g&l"
  ],
  "author": "heymanhn",
  "engines": {
    "node": "0.12.x"
  }
}
